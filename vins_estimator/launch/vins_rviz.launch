<launch>
    <node name="rvizvisualisation" pkg="rviz" type="rviz" output="log" args="-d $(find vins)/../config/vins_rviz_config.rviz" />
    <!-- Image conversion -->
    <node pkg="image_transport" type="republish" name="image_republish" args="compressed in:=/camera/left/image_rect raw out:=/camera/left/image_rect" output="screen" respawn="true"/>

</launch>
